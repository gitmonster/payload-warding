{
  "name": "payload-warding",
  "version": "0.1.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitmonster/payload-warding.git"
  },
  "description": "A Collections / Globals Backed Payload RBAC Plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "template",
    "rabc",
    "role"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "clean": "rimraf dist && rimraf dev/yarn.lock",
    "prepublishOnly": "yarn clean && yarn build && cd dev",
    "release": "release-it"
  },
  "author": "391861737@qq.com",
  "license": "MIT",
  "peerDependencies": {
    "payload": "^2.11.0"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "^0.0.1",
    "@swc/jest": "^0.2.28",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "dotenv": "^8.2.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.6.2",
    "payload": "^2.11.0",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "release-it": "^17.0.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "chore: Release v${version}",
      "pushRepo": "https://github.com/gitmonster/payload-warding.git"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "releaseName": "Release v${version}",
      "web": true,
      "autoGenerate": true
    }
  }
}
